{
  "name": "@srdjan/rendermaid",
  "version": "0.7.0",
  "description": "High-performance, functional TypeScript library for parsing and rendering Mermaid diagrams with optimized performance, multi-format output, and comprehensive type safety",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/srdjan/rendermaid.git"
  },
  "homepage": "https://github.com/srdjan/rendermaid",
  "bugs": {
    "url": "https://github.com/srdjan/rendermaid/issues"
  },
  "author": {
    "name": "Srdjan & Clody",
    "email": "team@rendermaid.dev"
  },
  "keywords": [
    "mermaid",
    "diagrams",
    "flowchart",
    "parser",
    "renderer",
    "svg",
    "html",
    "json",
    "typescript",
    "functional-programming",
    "ast",
    "visualization",
    "performance",
    "deno",
    "jsr",
    "immutable",
    "type-safe",
    "edge-routing",
    "spatial-grid"
  ],
  "exports": {
    ".": "./mod.ts",
    "./docs": "./docs.ts"
  },
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/rendermaid"
  },
  "publish": {
    "include": [
      "mod.ts",
      "docs.ts", 
      "lib/",
      "examples/",
      "tests/",
      "README.md",
      "LICENSE",
      "deno.json",
      "deno.jsonc",
      "jsr.json"
    ],
    "exclude": [
      "*.svg",
      "performance.ts",
      "main.ts", 
      "debug-*.ts",
      "baseline.txt",
      "node_modules/",
      ".git/",
      "dist/",
      "build/"
    ]
